    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Long-term Safety</measure>
    <time_frame>up to 24 weeks</time_frame>
    <description>Incidence of treatment-emergent adverse events following dosing with AXS-05.</description>
  </primary_outcome>
  <other_outcome>
    <measure>Cohen-Mansfield Agitation Inventory (CMAI)</measure>
    <time_frame>up to 24 weeks</time_frame>
    <description>Change from Baseline to Week 24 in the CMAI</description>
  </other_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">260</enrollment>
